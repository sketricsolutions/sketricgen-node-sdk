{
  "name": "sketricgen",
  "version": "0.1.0",
  "description": "Node.js SDK for Sketricgen workflows",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "sketricgen": "bin/cli.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "test": "node --test test/ 2>/dev/null || echo 'No tests yet'",
    "help": "node bin/cli.mjs --help"
  },
  "keywords": [
    "sketricgen",
    "workflow",
    "sdk"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}
